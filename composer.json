{
    "name": "lethality/activity",
    "description": "Easily retrieve a list of users and guests that are online.",
    "keywords": [
        "laravel",
        "php",
        "online activity"
    ],
    "homepage": "https://github.com/lethality/laravel-online-users",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Holmes",
            "email": "lee@lethality.me.uk",
            "homepage": "http://l3th4l1ty.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.2",
        "illuminate/session": "^5.2",
        "illuminate/database": "^5.2",
        "php" : ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Kim\\": "src/Kim"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lethality\\Activity\\ServiceProvider"
            ],
            "aliases": {
                "Activity": "Lethality\\Activity\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
